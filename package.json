{
  "name": "v-http",
  "description": "RxJS Vue Http Client",
  "main": "v-http.js",
  "private": true,
  "version": "0.0.1",
  "keywords": [
    "vue",
    "vuejs",
    "rxjs",
    "http",
    "request",
    "interceptors",
    "ajax",
    "fetch",
    "xhr"
  ],
  "author": {
    "name": "Luka M",
    "email": "lukam@nym.hush.com"
  },
  "type": "module",
  "license": "MIT",
  "typings": "types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --config vite.config.js && cp ./package.json dist/package.json && cp -r ./types dist",
    "build:package": "npm run build && npm pack ./dist/ --pack-destination ../v-http-module",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue-demi": "^0.13.11"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.0",
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "rxjs": "^7.5.6",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.5.0",
    "vue": "^3.2.39"
  }
}
